ictures/${pathname}.glb`);\n\nreturn (\n\n  <group ref={group} {...props} dispose={null}>\n  <mesh object={scene} {...props}  \n      castShadow\n      receiveShadow\n      geometry={nodes[pathname].geometry}\n      material={materials[material]}\n      rotation={randomObject[rotations]}\n      scale={randomObject[scales]} />\n\n  </group>\n)\n}\n\n// FRONT ART \n\nexport function Models() {\n  const controls = useRef()\n  return (\n    <div className=\"front-page_wrapper\">\n    <Canvas dpr={(1,2)} camera={{ position: [0, 4, 8], fov: 50 }} gl={{ alpha: false }}>\n      <Lights />\n      <Stars radius={100} depth={50} count={5000} factor={4} saturation={0} fade />\n      <Suspense fallback={null}>\n        <Stage controls={controls}>\n          <MagicMirror position={[-13, 3.5, 0]} rotation={[0, 0, 0]}>\n            <Lights />\n            <Sky sunPosition={[10000, 10, 10000]} />\n            <Angel fixedposition={[-.1, 59.9, 300]} scale={60} />\n          </MagicMirror>\n          <Beer scale={3.6} position={[-10.5, 0, 3]} rotation={[0, 1.30, 0]} />\n          <ObjectListLeft />\n         \n         \n          \n         \n          <Plant position={[-12.5, 0, 3]} />\n          \n         \n        </Stage>\n      </Suspense>\n      <OrbitControls ref={controls} />\n    </Canvas>\n    </div>\n  )\n}\n\nexport default Models\n\n// \n\n// Models \n\nexport function Beer(props) {\n  const group = useRef()\n  const { nodes, materials } = useGLTF('./about-pictures/14043_16_oz_Beer_Bottle_v2_L1.glb')\n  return (\n    <group ref={group} {...props} dispose={null}>\n      <mesh\n        castShadow\n        receiveShadow\n        geometry={nodes['14043_16_oz_Beer_Bottle_v2_L1'].geometry}\n        material={materials.Material__4}\n        rotation={[.03, -0.91, 0.03]}\n        scale={[0.1, 0.1, 0.1]}\n      />\n    </group>\n  )\n}\n\nexport function Concha(props) {\n  const group = useRef()\n  const { scene, nodes, materials } =useGLTF('./about-pictures/PAN_CONCHA001.glb')\